# Supabase Configuration - Production
NEXT_PUBLIC_SUPABASE_URL_PROD=           # Your production Supabase project URL
NEXT_PUBLIC_SUPABASE_ANON_KEY_PROD=      # Your production Supabase anon/public key
SUPABASE_URL_PROD=                       # Same as NEXT_PUBLIC_SUPABASE_URL_PROD
SUPABASE_KEY_PROD=                       # IMPORTANT: Must be the SERVICE ROLE KEY for production

# Supabase Configuration - Development
NEXT_PUBLIC_SUPABASE_URL_DEV=           # Your development Supabase project URL
NEXT_PUBLIC_SUPABASE_ANON_KEY_DEV=      # Your development Supabase anon/public key
SUPABASE_URL_DEV=                       # Same as NEXT_PUBLIC_SUPABASE_URL_DEV
SUPABASE_KEY_DEV=                       # IMPORTANT: Must be the SERVICE ROLE KEY for development

# Vector Store Configuration
VECTOR_DB_PASSWORD=
VECTOR_TABLE_NAME=message_embeddings_prod
VECTOR_JWT_SECRET=
NEXT_PUBLIC_VECTOR_SUPABASE_URL=
VECTOR_SUPABASE_URL=
VECTOR_SUPABASE_SERVICE_KEY=
NEXT_PUBLIC_VECTOR_SUPABASE_ANON_KEY=

# Tensai Configuration
NEXT_PUBLIC_TENSAI_KEY=
TENSAI_KEY=

# Environment
NODE_ENV=development
PORT=3000

# Build Configuration
NEXT_DISABLE_ESLINT=0                   # Enable in development for better feedback
NEXT_DISABLE_TYPE_CHECKS=0              # Enable in development for better feedback

# OpenAI Configuration
OPENAI_API_KEY=

# Vercel Configuration
CRON_SECRET=                            # Generated secure random string

# Langchain Configuration
LANGSMITH_TRACING=false
LANGSMITH_ENDPOINT=
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=

# Important Notes:
# 1. Never commit this file with real values
# 2. Make sure to set all required variables in your deployment environment
# 3. For local development, copy this file to .env.development and fill in values
# 4. For production, copy this file to .env.production and fill in values
# 5. NEXT_PUBLIC_ variables are exposed to the browser
# 6. SUPABASE_KEY_DEV should be service role key for local instance
# 7. SUPABASE_KEY_PROD should be service role key for production instance 